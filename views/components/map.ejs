<div class="container my-5 mx-auto pb-5 row">
    <div style="position: relative;">
        <div class="container">
            <div id="loader-custom" class="loader-custom d-none">
                <div class="spinner-border" role="status">
                </div>
            </div>
            <div id="map" style="width: auto; height: 800px;"></div>
            <div class="oficinas">
                <div class="buscador">
                    <i class="fa fa-search form-control-feedback"></i>
                    <input type="text" id="search-map" placeholder="Ingrese una Oficina">
                </div>
                <div class="menu-button" id="menu-button">
                    <img src="./images/menu.svg" alt="">
                </div>
                <div class="container-oficinas" id="container-oficinas">
                    <% mapInfo.forEach(info=> { %>
                    <div class="oficina my-3">
                        <div class="titulo-oficina">
                            <%= info.oficina_nombre %>
                        </div>
                        <hr />
                        <div class="cuerpo-oficina">
                            <div class="cuerpo-oficina-aforo">
                                <p> Aforo total: <%= info.oficina_aforo %>
                                </p>
                                <p style="font-weight: 700;"> Aforo disponible: <%= (info.oficina_aforo - info.customers) %>
                                </p>
                            </div>
                        </div>
                    </div>
                    <% }) %>
                </div>
            </div>
        </div>

        <div class="map-info-leyenda">
            <div class="map-info-leyenda-item">
                <span>Vacio</span>
                <div class="pl-2" style="margin-left: 0.2rem;height: 1rem; width:1rem;background-color: green;">
                </div>
            </div>
            <div class="map-info-leyenda-item">
                <span>Semi Lleno</span>
                <div class="pl-2" style="margin-left: 0.2rem;height: 1rem; width:1rem;background-color: yellow;">
                </div>
            </div>
            <div class="map-info-leyenda-item">
                <span>Lleno</span>
                <div class="pl-2" style="margin-left: 0.2rem;height: 1rem; width:1rem;background-color: red;"></div>
            </div>
        </div>
    </div>
</div>